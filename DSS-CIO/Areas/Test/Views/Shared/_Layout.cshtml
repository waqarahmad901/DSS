<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>DSS</title>
    <!-- Bootstrap -->
    <link href="~/Areas/Test/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/Areas/Test/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="~/Areas/Test/vendors/iCheck/skins/flat/green.css" rel="stylesheet">
    <!-- bootstrap-progressbar -->
    <link href="~/Areas/Test/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- jVectorMap -->
    <link href="~/Areas/Test/css/maps/jquery-jvectormap-2.0.3.css" rel="stylesheet" />
    <!-- Custom Theme Style -->
    <link href="~/Areas/Test/css/custom.css" rel="stylesheet">

    <!-- jQuery -->
    <script src="~/Areas/Test/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Areas/Test/vendors/bootstrap/dist/js/bootstrap.min.js"></script>

    <style type="text/css">
        table thead tr {
            background-color: skyblue;
        }
           #datatable122 thead tr, #datatable1272 thead tr {
            background-color: lightcoral;
        }
           #datatable122 tbody tr.odd,#datatable1272 tbody tr.odd {
            background-color: lightgreen !important;
        }
         #datatable122 tbody tr.even, #datatable1272 tbody tr.even {
            background-color:#f5dac6   !important;
        }
        #datatable122_wrapper .row:first-child, #datatable122_wrapper .row:last-child {
            display:none;
        }

        .odd {
            background-color:lightgray !important;
        }
        table {
            font-size:12pt;
            color:black;
            letter-spacing: 1px;
        }
        .main_container
        {
            background-color: royalblue;
        }
    </style>
</head>

<body class="nav-sm">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="index.html" class="site_title"><i class="fa fa-paw"></i> <span>DSS!</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">

                            <ul class="nav side-menu" >
                                <li>
                                    <a href="@Url.Action("Index")"><i class="fa fa-home"></i> Home </a>

                                </li>

                            </ul>
                        </div>

                    </div>

                </div>
            </div>

            <div class="top_nav">
                <div class="nav_menu" style="background-color: royalblue;">
                    <nav class="" role="navigation">
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" style="background-color: #D9DEE4; font-size:medium" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    Welcome <b>@HttpContext.Current.User.Identity.Name</b>
                                    <span class="fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">

                                    <li>
                                        <a href="@Url.Action("LogOff","Account")"><i class="fa fa-sign-out pull-right"></i> Log Out</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>

            <!-- page content -->
            <div class="right_col" role="main" style="height:800px">
                <div class="">
                    @RenderBody()
                </div>
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    Decision Support System For Chief Information Officer <a href="https://colorlib.com">MaxRemind</a>
                </div>
                <div class="clearfix"></div>
            </footer>

        </div>
    </div>

    <!-- ECharts -->
    <script src="~/Areas/Test/vendors/echarts/dist/echarts.min.js"></script>
    <script src="~/Areas/Test/vendors/echarts/map/js/world.js"></script>

    <!-- Custom Theme Scripts -->
    <script src="~/Areas/Test/js/custom.js"></script>

    <script src="~/Areas/Test/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-keytable/js/dataTables.keyTable.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
    <script src="~/Areas/Test/vendors/datatables.net-scroller/js/datatables.scroller.min.js"></script>


    <!-- Select2 -->
    <script src="~/Areas/Test/vendors/select2/dist/js/select2.full.min.js"></script>
    <!-- iCheck -->
    <script src="~/Areas/Test/vendors/iCheck/icheck.min.js"></script>
    <!-- FastClick -->
    <script src="~/Areas/Test/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Areas/Test/vendors/nprogress/nprogress.js"></script>
    <!-- validator -->
    <script src="~/Areas/Test/vendors/validator/validator.min.js"></script>

</body>
</html>